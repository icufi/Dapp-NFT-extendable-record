{
  "extends": ["airbnb", "prettier", "eslint:recommended", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
      "prettier/prettier": [
        "error",
        {"endOfLine": "auto"}
      ],
      "no-unused-vars": 1,
      "no-console": 0,
      "func-names": 0,
      "no-process-exit": 0,
      "object-shorthand": 0

  }
}
