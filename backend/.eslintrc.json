{
  "extends": [
    "airbnb",
    "prettier",
    "eslint:recommended",
    "plugin:node/recommended"
  ],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "prettier/prettier": [
      "error",
      { "endOfLine": "auto", "singleQuote": true }
    ],
    "no-unused-vars": 1,
    "no-console": 0,
    "func-names": 0,
    "no-process-exit": 0,
    "object-shorthand": 0,
    "consistent-return": 0,
    "no-unneeded-ternary": 0,
    "no-continue": 0,
    "no-await-in-loop": 0,
    "no-unsafe-finally": 0,
    "no-promise-executor-return": 0


  }
}
